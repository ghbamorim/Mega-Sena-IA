{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI Debug",
      "type": "python",
      "request": "launch",
      "module": "uvicorn", // Usa como m√≥dulo
      "args": [
        "app.main:app", // seu arquivo principal FastAPI
        "--reload", // hot reload para desenvolvimento
